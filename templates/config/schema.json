{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "array",
    "items": {
        "additionalProperties": true,
        "properties": {
            "key": {
                "type": "string",
                "uniqueItems": true
            },
            "value": {
                "type": "string"
            },
            "label": {
                "type": "string",
                "enum": [
                    "{{serviceName}}"
                ]
            },
            "content_type": {
                "type": "string"
            }
        },
        "required": [
            "key",
            "value",
            "label"
        ]
    }
}