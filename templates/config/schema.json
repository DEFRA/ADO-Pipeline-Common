{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "http://example.com/root.json",
    "type": "object",
    "properties": {
        "items": {
            "type": "array",
            "items": {
                "additionalProperties": false,
                "properties": {
                    "key": {
                        "type": "string",
                        "uniqueItems": true
                    },
                    "value": {
                        "type": "string"
                    },
                    "label": {
                        "type": "string",
                       "enum": [ "{{serviceName}}" ]
                    },
                    "content_type": {
                        "type": "string"
                    },
                    "tags": {
                        "type": "enum"
                    }
                },
                "required": [
                    "key",
                    "value",
                    "label"
                ]
            },
            "uniqueItems": true
        }
    }
}